{
  "name": "@tldraw/core",
  "version": "0.0.24",
  "description": "Core library and renderer for tldraw.",
  "author": "@steveruizok",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tldraw/tldraw.git",
    "directory": "packages/core"
  },
  "scripts": {
